+++
<% if (it.tags.length > 0) { %>
tags:
<% it.tags.forEach(function(tag) { %>
- "<%= tag.name %>"
<% }) %>
<% } %>
citable_urn: "<%= it.citable_urn %>"
target_urn: "<%= it.urn %>"
title: "<%= it.title %>"

authors:
<% it.authors.forEach(function(author) { %>
- email: "<%= author.email %>"
  name: "<%= author.name %>"
  username: "<%= author.username %>"
<% }) %>
+++

<%~ it.text %>
