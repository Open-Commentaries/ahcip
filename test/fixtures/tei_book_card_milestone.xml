  <teiHeader>
    <fileDesc>
      <titleStmt><title>Test Iliad Translation</title></titleStmt>
      <publicationStmt><p>Test</p></publicationStmt>
      <sourceDesc><p>Test source</p></sourceDesc>
    </fileDesc>
